{
  "name" : "StackOverFlow-Clip",
  "version" : "0.8",
  "description" : "Post StackOverFlow history",
  "permissions": [
    "notifications",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "http://localhost:3000/*",
    "background"
  ],
  "icons": { "16": "cookie.png", "48": "cookie.png", "128": "cookie.png" },
  "browser_action": {
    "default_icon": "cookie.png"
  },
  "web_accessible_resources": [
      "cookie.png",
      "img/1.png"
  ],
  "background": {
    "scripts": ["jquery.min.js", "background.js"]
  },
  "manifest_version": 2
}
